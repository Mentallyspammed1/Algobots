from typing import Union

Buffer = Union[bytes, bytearray, memoryview]

class SHA512Hash:
    digest_size: int
    block_size: int
    oid: str

    def __init__(self,
                 data: Buffer | None,
		 truncate: str | None) -> None: ...
    def update(self, data: Buffer) -> None: ...
    def digest(self) -> bytes: ...
    def hexdigest(self) -> str: ...
    def copy(self) -> SHA512Hash: ...
    def new(self, data: Buffer | None = ...) -> SHA512Hash: ...

def new(data: Buffer | None = ...,
        truncate: str | None = ...) -> SHA512Hash: ...
digest_size: int
block_size: int
