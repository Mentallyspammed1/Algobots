{
  "type": "module",
  "dependencies": {
    "@google/genai": "^1.13.0",
    "colorama": "^0.0.0",
    "dotenv": "^17.2.1",
    "mime": "^4.0.7"
  },
  "devDependencies": {
    "@types/node": "^24.2.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "prettier": "^3.0.0",
